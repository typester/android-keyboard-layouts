key COMMA {
    label:                              ','
    base:                               ','
    shift:                              '<'
}

key DPAD_LEFT {
   base:				                fallback DPAD_LEFT
   fn:				                	fallback MOVE_HOME
}

key DPAD_DOWN {
   base:			                	fallback DPAD_DOWN
   fn:			                		fallback PAGE_DOWN
}

key DPAD_RIGHT {
   base:		                		fallback DPAD_RIGHT
   fn:			                		fallback MOVE_END
}

# map TAB to Control
map key 15 CTRL_LEFT
# map Control to tab
map key 29 TAB

# PERIOD to COMMA
map key 52 COMMA
# UP to PERIOD
map key 103 PERIOD
# RSHIFT to slash
map key 54 SLASH

# COMMA to '
map key 51 APOSTROPHE
key APOSTROPHE {
    label:                              '\''
    base:                               '\''
    shift:                              '"'
    fn:                                 fallback DPAD_LEFT
}
# left to -
map key 105 MINUS
key MINUS {
    label:                              '-'
    base:                               '-'
    shift:                              '_'
    fn:                                 fallback DPAD_DOWN
}
# down to =
map key 108 EQUALS
key EQUALS {
    label:                              '='
    base:                               '='
    shift:                              '+'
    fn:                                 fallback DPAD_UP
}
# right to \
map key 106 BACKSLASH
key BACKSLASH {
    label:                              '\\'
    base:                               '\\'
    shift:                              '|'
    fn:                                 fallback DPAD_RIGHT
}

# APOSTOROPHE to SEMIcolon
map key 40 SEMICOLON
