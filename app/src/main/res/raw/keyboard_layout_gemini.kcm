#
# English (UK) keyboard layout.
# Unlike the default (generic) keyboard layout, English (US) does not contain any
# special ALT characters.
#

type OVERLAY

### ROW 1

key ESCAPE {
    base:                               fallback BACK
    alt, meta:                          fallback HOME
    ctrl:                               fallback MENU
    shift, capslock:                    '~'
}

key 1 {
    label:                              '1'
    base:                               '1'
    shift:                              '!'
    fn:			                		'~'
}

key 2 {
    label:                              '2'
    base:                               '2'
    shift:                              '@'
    fn:				                	'`'
}

key 3 {
    label:                              '3'
    base:                               '3'
    shift:                              '#'
}

key 4 {
    label:                              '4'
    base:                               '4'
    shift:                              '$'
}

key 5 {
    label:                              '5'
    base:                               '5'
    shift:                              '%'
    fn:		                			'<'
}

key 6 {
    label:                              '6'
    base:                               '6'
    shift:                              '^'
    fn:				                	'>'
}

key 7 {
    label:                              '7'
    base:                               '7'
    shift:                              '&'
    fn:			                		'['
}

key 8 {
    label:                              '8'
    base:                               '8'
    shift:                              '*'
    fn:			                		']'
}

key 9 {
    label:                              '9'
    base:                               '9'
    shift:                              '('
    fn:					                '{'
}

key 0 {
    label:                              '0'
    base:                               '0'
    shift:                              ')'
    fn:					                '}'
}

key DEL {
    base:			                	fallback DEL
    shift:        			            fallback FORWARD_DEL
}

### ROW 2

key Q {
    label:                              'Q'
    base:                               'q'
    shift, capslock:                    'Q'
}

key W {
    label:                              'W'
    base:                               'w'
    shift, capslock:                    'W'

}

key E {
    label:                              'E'
    base:                               'e'
    shift, capslock:                    'E'
}

key R {
    label:                              'R'
    base:                               'r'
    shift, capslock:                    'R'
}

key T {
    label:                              'T'
    base:                               't'
    shift, capslock:                    'T'
}

key Y {
    label:                              'Y'
    base:                               'y'
    shift, capslock:                    'Y'
    alt:                                '\u00fc'
    shift+alt, capslock+alt:            '\u00dc'
}

key U {
    label:                              'U'
    base:                               'u'
    shift, capslock:                    'U'
}

key I {
    label:                              'I'
    base:                               'i'
    shift, capslock:                    'I'
    fn:				                	'+'
}

key O {
    label:                              'O'
    base:                               'o'
    shift, capslock:                    'O'
    fn:				                	'-'
}

key P {
    label:                              'P'
    base:                               'p'
    shift, capslock:                    'P'
    fn:					                '='
}

key ENTER {
    label:                              '\n'
    base:                               '\n'
}

### ROW 3

key TAB {
    label:                              '\t'
    base:                               '\t'
}

key A {
    label:                              'A'
    base:                               'a'
    shift, capslock:                    'A'
}

key S {
    label:                              'S'
    base:                               's'
    shift, capslock:                    'S'
}

key D {
    label:                              'D'
    base:                               'd'
    shift, capslock:                    'D'
    alt:                                '\u00f0'
    shift+alt, capslock+alt:            '\u00d0'
}

key F {
    label:                              'F'
    base:                               'f'
    shift, capslock:                    'F'
}

key G {
    label:                              'G'
    base:                               'g'
    shift, capslock:                    'G'
}

key H {
    label:                              'H'
    base:                               'h'
    shift, capslock:                    'H'
}

key J {
    label:                              'J'
    base:                               'j'
    shift, capslock:                    'J'
    fn:					                '_'
}

key K {
    label:                              'K'
    base:                               'k'
    shift, capslock:                    'K'
    fn:					                ';'
}

key L {
    label:                              'L'
    base:                               'l'
    shift, capslock:                    'L'
    fn:				                	'"'
}

